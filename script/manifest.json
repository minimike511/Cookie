{
  "manifest_version": 2,

  "name": "Random Hijack Script",
  "description": "Install at your own risk :P",
  "version": "1.0",
  "author": "NobodyRandom",

  "content_scripts": [
	{
	  "matches": [
		"*://owl.uwo.ca/*"
	  ],
	  "include_globs": [
		"https://owl.uwo.ca/*"
	  ],
	  "js": [
		"injector.js"
	  ],
	  "run_at": "document_end",
	  "all_frames": true
	}
  ],

  "web_accessible_resources": [
	"MouseHunt%20AutoBot.user.js",
	  "parse-1.6.14.min.js",
	"https://github.com/nobodyrandom/mhAutobot/raw/master/tamperMonkey/MouseHunt%20AutoBot.user.js"
  ],

  "permissions": [
	"activeTab",
	"tabs",
	"unlimitedStorage",
	"storage"
  ]
}
